<h1>All Shelters</h1>

<% @shelters.each do |shelter| %>
  <h2><%= link_to "#{shelter.name}", "/shelters/#{shelter.id}" %></h2>
  <%= link_to "Edit Shelter #{shelter.name}", "/shelters/#{shelter.id}/edit" %>
  <%= link_to "Delete Shelter #{shelter.name}", "/shelters/#{shelter.id}", method: :delete %>
<% end %>

<br/><br/><%= link_to "New Shelter", "/shelters/new" %>
